<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="report_ce_document_stock_action" model="ir.actions.report">
    <field name="name">Hoja CE por entrega</field>
    <field name="model">stock.move.line</field>
    <field name="report_type">qweb-pdf</field>
    <field name="report_name">custom_ce_template.report_ce_document_stock</field>
    <field name="file">custom_ce_template.report_ce_document_stock</field>
    <field name="print_report_name">'CE_%s_%s' % (object.product_id.name, object.lot_id.name or 'NOLot')</field>
  </record>
</odoo>
