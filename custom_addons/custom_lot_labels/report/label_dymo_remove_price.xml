<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_simple_label_dymo_noprice" inherit_id="product.report_simple_label_dymo">

        <!-- Eliminar precio -->
        <xpath expr="//strong[hasclass('o_label_price_small')]" position="replace">
            <!-- Precio eliminado: no visible en la etiqueta -->
        </xpath>

        <!-- Añadir ubicación temporal directamente desde product.template -->
        <xpath expr="//div[hasclass('o_label_full')]" position="inside">
            <div t-if="product.x_ubicacion_temporal"
                style="font-size:10px; text-align:right; margin-top:4px;">
                <t t-esc="product.x_ubicacion_temporal"/>
            </div>
        </xpath>

    </template>
</odoo>
